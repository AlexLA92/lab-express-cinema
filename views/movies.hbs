<h1>Movies list</h1>
<div class = "movies-container">
    {{#each movies}}    
        <div class = "movie-container">
            <img src="{{this.image}}" alt="{{this.title}}">
            <h2>{{this.title}}</h2>
            <p><a href="/movies/{{this._id}}">See more</a></p>
        </div>
    {{/each}}
</div>